{
  "name": "smartsolutionsitaly/cakephp-twitter",
  "description": "Twitter client and helpers for CakePHP",
  "keywords": [
    "cakephp",
    "twitter",
    "client"
  ],
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "abraham/twitteroauth": "^0.7.4"
  },
  "require-dev": {
    "cakephp/cakephp": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "SmartSolutionsItaly\\CakePHP\\Twitter\\": "src"
    }
  },
  "autoload-dev": {
